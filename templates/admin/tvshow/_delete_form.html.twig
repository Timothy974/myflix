<form method="post" action="{{ path('tv_show_delete', {'id': tvshow.id}) }}" onsubmit="return confirm('Êtes-vous sûr de vouloir supprimer cet élément ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ tvshow.id) }}">
    <button class="deleteBtn"><i class="fa-solid fa-trash-can"></i></button>
</form>
